<div class="form-group">
  <label for="inputPassword3" class="col-sm-3 control-label">Default Series</label>
  <div class="col-sm-9">
    <%= form.text_field :series, class: "form-control" %>
    <span class="help-block">Example: [{"name":"Instruments","data":[8601,843,786,781,692,608,534,435,111,37]}]</span>
  </div>
</div>
<div class="form-group">
  <label for="inputPassword3" class="col-sm-3 control-label">Default Categories</label>
  <div class="col-sm-9">
    <%= form.text_field :categories, class: "form-control" %>
    <span class="help-block">Example: ["BEATBOX", "PIANO", "VIOLA", "FLUTE", "CLAPPING", "BANJO", "UKULELE", "ABLETON", "DRUMS", "GUITAR"] </span>
  </div>
</div>


